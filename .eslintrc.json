{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:react/recommended", "plugin:prettier/recommended", "next/core-web-vitals"],
	"ignorePatterns": ["*.js", "*.jsx", "*.mjs"],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"@typescript-eslint/no-explicit-any": 0,
		"eslint-plugin-import/no-anonymous-default-export": 0,
		"eslint-plugin-import/no-cycle": 0,
		"eslint-plugin-import/no-deprecated": 0,
		"eslint-plugin-import/no-duplicates": 0,
		"eslint-plugin-import/no-dynamic-require": 0,
		"eslint-plugin-import/no-internal-modules": 0,
		"eslint-plugin-import/no-named-as-default": 0,
		"eslint-plugin-import/no-named-as-default-member": 0,
		"eslint-plugin-import/no-named-default": 0,
		"eslint-plugin-import/no-relative-parent-imports": 0,
		"eslint-plugin-import/no-self-import": 0,
		"reportUnusedDisableDirectives": 0,
		"eslint-plugin-import/no-unassigned-import": 0,
		"eslint-plugin-import/no-unused-modules": 0,
		"eslint-plugin-import/no-useless-path-segments": 0,
		"eslint-plugin-import/no-webpack-loader-syntax": 0,
		"react/display-name": 0,
		"react/jsx-no-script-url": 0,
		"react/jsx-no-target-blank": 0,
		"react/no-array-index-key": 0,
		"react/no-children-prop": 0,
		"react/no-danger": 0,
		"react/no-danger-with-children": 0,
		"react/no-deprecated": 0,
		"react/no-direct-mutation-state": 0,
		"react/no-redundant-should-component-update": 0,
		"react/no-this-in-sfc": 0,
		"react/no-typos": 0,
		"react/no-unescaped-entities": 0,
		"react/no-unsafe": 0,
		"react/prop-types": 0,
		"react/react-in-jsx-scope": 0,
		"prettier/prettier": 0,
		"typescript-eslint/ban-ts-comment": 0
	}
}
